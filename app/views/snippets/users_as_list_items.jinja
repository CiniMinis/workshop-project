    <canvas class="avatar p-0 m-0" data-user-id='{{user.user_id}}'></canvas>
    <!-- <div class="container w-100 p-2 m-0"> -->
        <h5 class="w-100 bg-dark color-dark p-2 m-0 h-25">
            {{user.name}}
        </h5>
    <!-- </div> -->
</a>
{% endfor %}